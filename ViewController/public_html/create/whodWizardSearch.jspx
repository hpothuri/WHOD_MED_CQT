                                            <af:outputText value="#{row.ReleaseStatus}" id="ot6"/>
                                        </af:column>
                                        <af:column sortProperty="RelaeaseGroup" sortable="true"
                                                   headerText="Release Group" id="c10" width="80" displayIndex="-1"
                                                   filterable="true">
                                            <af:outputText value="#{row.RelaeaseGroup}" id="ot3"/>
                                        </af:column>
                                        <af:column sortProperty="Value4" sortable="true" headerText="Group"
                                                   id="c15" width="60" filterable="true">
                                            <af:outputText value="#{row.Value4}" id="ot20"/>
                                        </af:column>
                                        <af:column sortable="true" headerText="Level" id="c18" width="40"
