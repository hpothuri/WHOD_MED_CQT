<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="ia-publish-task-flow-definition">
    <default-activity>init</default-activity>
    <managed-bean id="__12">
      <managed-bean-name>WorkflowBean</managed-bean-name>
      <managed-bean-class>com.dbms.csmq.view.backing.publish.WorkflowBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__3">
      <managed-bean-name>CSMQBean</managed-bean-name>
      <managed-bean-class>com.dbms.csmq.CSMQBean</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <view id="promoteToPublished">
      <page>/workflow/ia_promoteToPublished.jspx</page>
      <train-stop>
        <display-name>Promote To Published</display-name>
        <sequential>false</sequential>
      </train-stop>
    </view>
    <view id="demoteToDraft">
      <page>/workflow/ia_demoteToDraft.jspx</page>
      <train-stop>
        <display-name>Demote To Pending Impact Assessment</display-name>
        <sequential>false</sequential>
      </train-stop>
    </view>
    <view id="activate">
      <page>/workflow/ia_activate.jspx</page>
    </view>
    <method-call id="clearMenu">
      <method>#{UserBean.clear}</method>
      <outcome id="__44">
        <fixed-outcome>CLEARED</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="cancelFlow">
      <outcome id="__15">
        <name>cancelFlow</name>
      </outcome>
    </task-flow-return>
    <method-call id="init">
      <method>#{pageFlowScope.WorkflowBean.IA_init}</method>
      <outcome id="__31">
        <to-string/>
      </outcome>
    </method-call>
    <train/>
    <control-flow-rule id="__1">
      <from-activity-id>promoteToPublished</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>CANCEL</from-outcome>
        <to-activity-id>clearMenu</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>demoteToDraft</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>CANCEL</from-outcome>
        <to-activity-id>clearMenu</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>activate</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>CANCEL</from-outcome>
        <to-activity-id>clearMenu</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__9">
      <from-activity-id>clearMenu</from-activity-id>
      <control-flow-case id="__10">
        <from-outcome>CLEARED</from-outcome>
        <to-activity-id>cancelFlow</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__11">
      <from-activity-id>init</from-activity-id>
      <control-flow-case id="__13">
        <to-activity-id>promoteToPublished</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
